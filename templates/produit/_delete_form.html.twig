<form class="form-inline" method="post" action="{{ path('produit_delete', {'id': produit.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ produit.id) }}">
    <button class="btn">Supprimer</button>
</form>
